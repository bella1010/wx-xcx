/**app.wxss**/
@import 'style/weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
image {
 width: 320px;
 height: 240px;
 display: inline-block;
 overflow: hidden;
}
page{
    background-color: #f5f5f5;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__bd{
    padding: 10px;
}
.page__bd_spacing{
    padding-bottom:60px;
}
.page__bd_marspacing{
    margin-bottom:10px;
}
.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    padding-top: 20px;
    text-align: center;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: center;
    font-size: 14px;
   display:none;
}



.probrand{
    width:80px;
    height:26px; 
}
.protitle{
    font-size:14pt;
    font-weight: bold;
    color: #353535
}
.proprice{
    color:#f00;
    font-size:11pt;
}
.probrandT,
.prodescribe{
    font-size:10pt;
    color: #858585
}
.proprice,
.pronumber{
    color:#f70;
}
.propriceold{
    text-decoration: line-through;
    margin: 0 5px;
    font-size:9pt;
    color: #888888;
}


.pronumber{
    float: right
}
.pronumber text{
    float: left;
}
.pronumberbtn{
    width:30px;
    height:30px;
    text-align: center;
    line-height: 30px;
    padding:0;
    float: left;
    border-radius: 0;
    border:0;
    background: #eee; 
    color: #555555
}
.pronumberbtn:after{
    border-radius: 0;
    border:0;
}
.pronumberinput{
    width:40px; 
    height:30px;
    border:0;
    background: #eee;    
    text-align: center;
    float: left;
    margin: 0 1px;
}



.page-loading{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 14px;
  color: #666;
}
.page-loading .loading-text{
  display: inline-block;
  vertical-align: middle;
}
.weui-loading {
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.load-more-wrap{
  text-align: center;
  padding: 10px 20px 20px 20px;
}
.load-content{
  padding: 5px;
  color: #666;
}
.btn-load-more text{
  display: block;
  padding: 5px;
  border-radius: 3px;
  background: #e6e6e6;
  color: #666;
  text-align: center;
}



/* 字体图标 */
@font-face {
    font-family: 'iconfont';  /* project id："163415" */
    src: url('//at.alicdn.com/t/font_oai4zufu2m3ac3di.eot');
    src: url('//at.alicdn.com/t/font_oai4zufu2m3ac3di.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_oai4zufu2m3ac3di.woff') format('woff'),
    url('//at.alicdn.com/t/font_oai4zufu2m3ac3di.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_oai4zufu2m3ac3di.svg#iconfont') format('svg');
}

.iconfont {
    font-family:"iconfont" !important;
    font-size:32rpx;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.4rpx;
    -moz-osx-font-smoothing: grayscale;
}

